{
  "name": "vuex-easy-firestore",
  "version": "1.29.2",
  "description": "Easy coupling of firestore and a vuex module. 2-way sync with 0 boilerplate!",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "ava --verbose",
    "rollup": "rollup -c build/rollup.js",
    "rollup:watch": "rollup -c build/rollup.js --watch",
    "build": "npm run rollup && npm run test",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./docs/.vuepress/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/vuex-easy-firestore.git"
  },
  "keywords": [
    "vuex",
    "firestore",
    "firebase",
    "boilerplate",
    "store",
    "vue"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/vuex-easy-firestore/issues"
  },
  "homepage": "https://github.com/mesqueeb/vuex-easy-firestore#readme",
  "dependencies": {
    "compare-anything": "0.0.2",
    "copy-anything": "^1.2.1",
    "filter-anything": "^1.1.1",
    "find-and-replace-anything": "^2.0.1",
    "flatten-anything": "^1.0.1",
    "is-what": "^3.2.1",
    "merge-anything": "^2.2.3",
    "vuex-easy-access": "^3.1.5"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "ava": "^1.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "firebase": "^5.8.6",
    "mock-cloud-firestore": "^0.9.2",
    "rollup-plugin-typescript2": "^0.20.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.1.1",
    "vue": "^2.6.8",
    "vuepress": "^0.14.10",
    "vuex": "^3.1.0"
  },
  "peerDependencies": {
    "firebase": "^5.8.0"
  }
}
